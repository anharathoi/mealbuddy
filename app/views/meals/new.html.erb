<h1>Meals#new</h1>



<%= form_with model: @meal do |form| %>
  <p>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </p>

  <p>
    <%= form.label :description %><br>
    <%= form.text_area :description %>
  </p>
 
<p>
    <%= form.label :price %><br>
    <%= form.number_field :price %>
  </p>

<p>
    <%= form.label :available_from %><br>
    <%= form.datetime_local_field :available_from %>
  </p>

<p>
    <%= form.label :available_to %><br>
    <%= form.datetime_local_field :available_to %>
  </p>





  <p>
    <%= form.submit %>
  </p>
<% end %>
<%= link_to 'Back',root_path %>



